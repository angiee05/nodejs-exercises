{
  "name": "createyourownhttpserver",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc",
    "build:dev": "tsc --watch",
    "start": "node --enable-source-maps dist/server.js",
    "start:dev": "nodemon --watch dist/ --watch .env",
    "test": "jest",
    "dev": "npm run build && concurrently 'npm:build:dev' 'npm:start:dev'",
    "web": "serve -p 8080 web/"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.1.1",
    "@sinclair/typebox": "^0.24.27",
    "@types/multer": "^1.4.7",
    "ajv-formats": "^2.1.1",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-json-validator-middleware": "^3.0.1",
    "express-session": "^1.17.3",
    "mime": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.6.0",
    "passport-github2": "^0.1.12"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
