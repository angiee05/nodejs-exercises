    "@types/express-session": "^1.17.5",
    "@types/jest": "^28.1.6",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.6.2",
    "@types/passport": "^1.0.10",
    "@types/passport-github2": "^1.2.5",
    "@types/supertest": "^2.0.12",
    "concurrently": "^7.3.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "prisma": "^4.1.1",
    "serve": "^14.0.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  }
